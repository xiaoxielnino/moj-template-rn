{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean:cache": "rm -fr $TMPDIR/metro*",

    "android": "npm run clean:cache && react-native run-android",
    "build:android:test": "node ./build --android:test",
    "build:android": "node ./build --android",
    "ios": "npm run clean:cache && react-native run-ios",
    "build:ios:test": "node ./build --ios:test",
    "build:ios": "node ./build --ios",

    "start": "react-native start",

    "lint": "run-p lint:*",
    "lint:js": "elint lint es \"**/*.js\" \"**/*.jsx\"",
    "lint:css": "elint lint style \"**/*.css\" \"**/*.less\"",
    "fix": "run-p fix:*",
    "fix:js": "elint lint es \"**/*.js\"  \"**/*.jsx\" --fix",
    "fix:css": "elint lint style \"**/*.css\" \"**/*.less\" --fix",
    "beforecommit": "elint lint commit && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/runtime": "^7.6.2",
    "@music/elint-preset-base": "^1.0.13",
    "babel-eslint": "^10.1.0",
    "elint": "^1.11.1",
    "metro-react-native-babel-preset": "^0.58.0",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3"
  },
  "dependencies": {
    "@music/rn-util": "^0.0.12",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/native": "^5.1.5",
    "@react-navigation/stack": "^5.2.10",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-native": "^0.62.2",
    "react-native-screens": "2.4.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-svg": "^12.0.3",
    "react-redux": "^5.1.2",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^5.1.1"
  }
