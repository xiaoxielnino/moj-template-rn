{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "react-native start -- --reset-cache",
    "clean:cache": "rm -fr $TMPDIR/metro*",
    "start:web": "webpack-dev-server --config ./webpack.client.js --mode development --colors --progress",
    "build:web": "webpack --config ./webpack.client.js --mode production --colors --progress",
    "analysis:web": "webpack --config ./webpack.client.js --mode production --colors --progress --env.analysis",
    "build:web:test": "webpack --config web/webpack/webpack.config.js --env.test",
    "build:ios:test": "node ./build --ios:test",
    "build:android:test": "node ./build --android:test",
    "build:ios": "node ./build --ios",
    "build:android": "node ./build --android",
    "lint": "run-p lint:*",
    "lint:js": "elint lint es \"**/*.js\" \"**/*.jsx\"",
    "lint:css": "elint lint style \"**/*.css\" \"**/*.less\"",
    "fix": "run-p fix:*",
    "fix:js": "elint lint es \"**/*.js\"  \"**/*.jsx\" --fix",
    "fix:css": "elint lint style \"**/*.css\" \"**/*.less\" --fix",
    "beforecommit": "elint lint commit && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
